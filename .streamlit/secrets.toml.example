# Exemplo de arquivo de secrets para Streamlit
# Copie este arquivo para secrets.toml e adicione suas credenciais reais
# O arquivo secrets.toml está no .gitignore e não será commitado

[aws]
access_key_id = "SUA_AWS_ACCESS_KEY_ID"
secret_access_key = "SUA_AWS_SECRET_ACCESS_KEY"
region = "eu-west-1"

# Exemplo de uso no código:
# aws_key = st.secrets["aws"]["access_key_id"]
# aws_secret = st.secrets["aws"]["secret_access_key"]
